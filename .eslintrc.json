{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "eslint:recommended"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "react/function-component-definition": [
            2,
            {
              "namedComponents": "arrow-function"
            },
            {
                "jsx": "always"
            },
            {
                "js": "always"
            }
          ]
    }
}
