{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/extensions": [
            2,
            {
                "jsx": "always"
            }
        ],
        "react/function-component-definition": [
            2,
            {
              "namedComponents": "arrow-function"
            }
        ],
        "react/prop-types": [
            0,
            {}
        ]
    }
}
